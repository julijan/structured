{
  "type": "commonjs",
  "scripts": {
    "buildAssetsJS": "tsc assets/ts/* --outDir assets/js/",
    "build": "tsc && npm run buildAssetsJS",
    "developAssets": "tsc --watch assets/ts/* --outDir assets/js/",
    "develop": "tsc --watch",
    "start": "cd build && node index.js",
    "startDev": "cd build && nodemon index.js"
  },
  "devDependencies": {
    "@types/jsdom": "^20.0.1",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^18.11.9"
  },
  "dependencies": {
    "handlebars": "^4.7.7",
    "jsdom": "^20.0.2",
    "mime-types": "^2.1.35"
  }
}
